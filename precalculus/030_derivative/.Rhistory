fixer = function(exp){
exp = gsub("+-","-",exp,fixed=T)
exp = gsub("--","+",exp,fixed=T)
exp = gsub("t^0","",exp,fixed=T)
exp = gsub("t^1","x",exp,fixed=T)
return(exp)
}
while(T){
FC = 10^runif(1,3,6)  # Fixed cost of equipment
U = 10^runif(1,0,3)
ndoubcost = 10^runif(1,3,5)
A = FC/ndoubcost^3
B = U*runif(1,1,100)
nrev0 = ndoubcost*runif(1,0.01,100)
C = B/nrev0
FC = signif(FC,2)
U = signif(U,2)
A = signif(A,2)
B = signif(B,2)
C = signif(C,2)
nstar = sqrt((B-U)/(3*A+3*C))
Pstar = -A*nstar^3-C*nstar^3+(B-U)*nstar-FC
if(A>0.001 && nstar>100 && Pstar>1000 && nstar<nrev0){break}
}
