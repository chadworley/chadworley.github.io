f = function(x){
return(x^3-x)
}
g = function(x,t){
return(f(x+t))
}
h = function(x,t){
return(f(x)+t)
}
m = function(x,t){
return(f(t*x))
}
n = function(x,t){
return(t*f(x))
}
t = c(seq(-10,10,2),seq(10,-10,-2))
x = seq(-50,50,0.01)
gt = list()
ht = list()
mt = list()
nt = list()
for(i in 1:length(t)){
gt[[i]] = g(x,t[i])
ht[[i]] = h(x,t[i])
mt[[i]] = m(x,t[i])
nt[[i]] = n(x,t[i])
}
lis = list(gt,ht,mt,nt)
